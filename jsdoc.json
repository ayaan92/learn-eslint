{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.jsx?$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/tui-jsdoc-template",
    "destination": "./docs",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}
