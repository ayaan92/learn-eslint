{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.jsx?$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "recurse": true,
    "destination": "docs"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}
